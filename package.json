{
  "name": "functions",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "config:unset": "firebase functions:config:unset env",
    "config:set:personal": "yarn config:unset && firebase functions:config:set env=\"$(cat env.personal.json)\" || echo \"Please add the file env.json before deploy.\"",
    "config:get": "firebase functions:config:get",
    "build": "yarn lint && rm -rf ./lib && tsc",
    "serve": "yarn build && firebase serve --only functions",
    "shell": "yarn build && firebase functions:shell",
    "start": "yarn shell",
    "lint": "eslint --fix -c ./.eslintrc.json './**/*.ts'",
    "logs": "firebase functions:log",
    "simulate": "yarn build & firebase emulators:start --only functions",
    "config:set:emulator": "firebase functions:config:get > .runtimeconfig.json",
    "deploy": "firebase deploy --only functions",
    "hello": "node ./src/hello.js"
  },
  "engines": {
    "node": "12"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/storage": "^5.0.0",
    "cors": "^2.8.5",
    "csv2json": "^2.0.2",
    "dayjs": "^1.8.26",
    "express": "^4.17.1",
    "express-http-context": "^1.2.3",
    "firebase": "^8.0.1",
    "firebase-admin": "^8.9.0",
    "firebase-functions": "^3.3.0",
    "json2csv": "^5.0.1",
    "mysql2": "^2.0.1",
    "promise-mysql": "^4.1.3",
    "request-ip": "^2.1.3",
    "sequelize": "^5.21.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/firebase": "^3.2.1",
    "@types/jest": "^25.2.1",
    "@types/json2csv": "^5.0.0",
    "@types/request-ip": "^0.0.34",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-jest": "^25.4.0",
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "firebase-functions-test": "^0.2.1",
    "husky": "^3.0.4",
    "jest": "^25.4.0",
    "lint-staged": "^9.2.4",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "author": "Shinya Takahahi",
  "license": "ISC"
}
